{
    "name": "bartdezwaan/event-sourcing",
    "license": "MIT",
    "type": "library",
    "description": "async handling of broadway events",
    "autoload": {
        "psr-4": {
            "Zwaan\\EventSourcing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zwaan\\EventSourcing\\": "test/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "broadway/broadway": ">=0.5.0",
        "videlalvaro/php-amqplib": "2.5.*",
        "jms/serializer": "^1.1",
        "zumba/json-serializer": "^1.0",
        "guzzlehttp/guzzle": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.7",
        "doctrine/dbal": "~2.4"
    },
    "config": {
        "bin-dir": "bin"
    }
}
